package com.bhester;

import java.io.Serializable;

public class Photo implements Serializable {

	private int photoID;
	private String photoFileName;
	private int dateOfPhoto;
	private String descriptionOfPhoto;
	
	public Photo(int photoID, String photoFileName, int dateOfPhoto, String descriptionOfPhoto) {
		super();
		this.photoID = photoID;
		this.photoFileName = photoFileName;
		this.dateOfPhoto = dateOfPhoto;
		this.descriptionOfPhoto = descriptionOfPhoto;
	}

	@Override
	public String toString() {
		return "Photo [photoID=" + photoID + ", photoFileName=" + photoFileName + ", dateOfPhoto=" + dateOfPhoto
				+ ", descriptionOfPhoto=" + descriptionOfPhoto + "]";
	}

	public int getPhotoID() {
		return photoID;
	}

	public void setPhotoID(int photoID) {
		this.photoID = photoID;
	}

	public String getPhotoFileName() {
		return photoFileName;
	}

	public void setPhotoFileName(String photoFileName) {
		this.photoFileName = photoFileName;
	}

	public int getDateOfPhoto() {
		return dateOfPhoto;
	}

	public void setDateOfPhoto(int dateOfPhoto) {
		this.dateOfPhoto = dateOfPhoto;
	}

	public String getDescriptionOfPhoto() {
		return descriptionOfPhoto;
	}

	public void setDescriptionOfPhoto(String descriptionOfPhoto) {
		this.descriptionOfPhoto = descriptionOfPhoto;
	}
	
	
}
